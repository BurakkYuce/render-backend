{
  "name": "rentaly-backend",
  "version": "1.0.0",
  "description": "Rentaly Car Rental Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/controllers tests/models",
    "test:integration": "jest tests/integration",
    "test:load": "RUN_LOAD_TESTS=true jest tests/load",
    "test:all": "node tests/runTests.js",
    "test:controllers": "node tests/runTests.js --suite=controllers",
    "test:models": "node tests/runTests.js --suite=models",
    "test:api": "node tests/runTests.js --suite=integration",
    "seed": "node src/utils/seedDatabase.js"
  },
  "keywords": [
    "car-rental",
    "api",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "express": "^4.19.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.17.2",
    "mongoose": "^8.16.5",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "form-data": "^4.0.4",
    "jest": "^30.0.5",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
